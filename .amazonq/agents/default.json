{
  "name": "q_ide_default",
  "description": "Default agent configuration",
  "prompt": "",
  "mcpServers": {
    "aws-mcp": {
      "command": "uvx",
      "disabled": false,
      "timeout": 60000,
      "args": [
        "mcp-proxy-for-aws@latest",
        "https://aws-mcp.us-east-1.api.aws/mcp"
      ],
      "env": {
        "AWS_REGION": "us-east-1"
      }
    }
  },
  "tools": [
    "fs_read",
    "execute_bash",
    "fs_write",
    "report_issue",
    "use_aws",
    "@aws-mcp"
  ],
  "toolAliases": {},
  "allowedTools": [
    "fs_read",
    "report_issue",
    "use_aws",
    "execute_bash",
    "fs_write"
  ],
  "toolsSettings": {
    "use_aws": {
      "preset": "readOnly"
    },
    "execute_bash": {
      "preset": "readOnly"
    }
  },
  "resources": [
    "file://AmazonQ.md",
    "file://README.md",
    "file://.amazonq/rules/**/*.md"
  ],
  "hooks": {
    "agentSpawn": [],
    "userPromptSubmit": []
  },
  "useLegacyMcpJson": true
}